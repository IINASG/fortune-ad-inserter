{
    "name": "iinasg/fortune-ad-inserter",
    "description": "",
    "keywords": [
        "iinasg",
        "fortune-ad-inserter"
    ],
    "homepage": "https://github.com/iinasg/fortune-ad-inserter",
    "license": "MIT",
    "authors": [
        {
            "name": "Koji Suzuki",
            "email": "k.suzuki@ii-na.jp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts" : "^8.0||^9.0||^10.0||^11.0"
    },
    "require-dev": {
        "fakerphp/faker" : "^1.23",
        "laravel/tinker" : "^2.9",
        "roave/security-advisories": "dev-latest",
        "symfony/dom-crawler": "^5.3",
        "orchestra/testbench" : "^6.0||^7.0||^8.0||^9.0",
        "orchestra/testbench-dusk" : "^6.0||^7.0||^8.0||^9.0",
        "phpunit/phpunit" : "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Iinasg\\FortuneAdInserter\\": "src",
            "Database\\Factories\\Iinasg\\FortuneAdInserter\\Models\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iinasg\\FortuneAdInserter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iinasg\\FortuneAdInserter\\FortuneAdInserterServiceProvider"
            ],
            "aliases": {
                "FortuneAdInserter": "Iinasg\\FortuneAdInserter\\Facades\\FortuneAdInserterFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
